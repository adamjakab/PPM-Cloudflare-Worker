{
  "name": "cloudflare-worker-tests",
  "version": "0.0.1",
  "description": "My first CF Worker in TS",
  "main": "src/index.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "lint-fix": "tslint --project tsconfig.json --fix",
    "test": "jest",
    "test-tmp": "jest --config=jest.tmp.js",
    "build": "webpack --config webpack.config.js --progress # Create minified output",
    "build-dev": "webpack --config webpack.development.js --display-modules # Creates readable output",
    "build-all": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamjakab/CF-Worker-Tests.git"
  },
  "keywords": [
    "typescript",
    "cloudflare",
    "workers"
  ],
  "author": "Adam Jakab <adam@jakab.pro>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.162",
    "@types/node": "^12.12.17",
    "@udacity/types-service-worker-mock": "^1.1.0",
    "cloudflare-worker-mock": "^1.1.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "types-cloudflare-worker": "^1.1.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@types/url-parse": "^1.4.3",
    "cloudflare-worker-rest-api": "^1.0.0",
    "lodash": "^4.17.15",
    "typed-routes": "^0.1.1",
    "url-parse": "^1.4.7"
  }
}
